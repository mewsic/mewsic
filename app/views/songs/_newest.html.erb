<div id="song_list_box">
		<div class="song_lists_head fright">
			<p class="pick_info block">
				<strong>NEWEST SONGS</strong>
			</p>
			<span class="subtitle block">Discover Mewsicians last creations</span>
		</div>
		<div class="clear"> </div>

		<div class="browse_pane pane_content">
			<div class="browse_pane_left fleft">
				<div class="loader"> </div>
				<ul class="instr_type alright">
					<% @newest_dates.each do |label, day| %>
						<li class="<%= 'current' if day.today? rescue false %>"><%= link_to label, "#newest/#{day}" %></li>
					<% end %>
				</ul>
			</div>
			<div class="browse_pane_right fleft">
				<ul class="maingenre_type alleft">
					<li class="current"><a href="">All</a></li>
					<% @newest_tags[0..6].each do |tag| %>
						<li><%= link_to tag, "#newest/#{e(tag)}" %></li>
					<% end %>
				</ul>
				<div id="song_newest_list" class="fleft">
          <%= render :partial => 'shared/song_block', :collection => @newest_songs %>
				</div>
        <%= render :partial => 'shared/bullet_paging' %>
				
			</div>
		</div>
</div>
<div class="clear"> </div>
