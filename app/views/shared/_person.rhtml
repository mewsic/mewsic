<div class="elements clear-block"> 
  <p class="star">
    <%= stars(5, color) %>
  </p>
  <p class="thumb">
    <a href="<%= user_path person %>"><img src="/avatars/photo_60X60.jpg" alt="<%= person.login %>" width="60" height="60" /></a>
  </p>
  <p class="name"><a href="<%= user_path person %>"><%= person.login %></a></p>
  <p class="country"><%= h person.compiled_location%></p>
</div>
