<div class="elements clear-block"> 
  <p class="star">
    <%= stars(5, color) %>
  </p>
  <p class="thumb">
    <a href="#"><img src="/avatars/photo_60X60.jpg" alt="<%= person.login %>" width="60" height="60" /></a>
  </p>
  <p class="name"><a href="#"><%= person.login %></a></p>
  <p class="country"><%= person.compiled_location || "Nowhere" %></p>
</div>
