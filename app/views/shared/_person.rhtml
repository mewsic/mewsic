<div class="elements clear-block"> 
  <p class="star">
    <%= stars(person.rating_avg, color) %>
  </p>
  <p class="thumb">
    <%= link_to avatar_image(person, :medium), person %>
  </p>
  <p class="name"><%= link_to person.login, person %></p>
  <p class="country"><%= h person.compiled_location%></p>
</div>
