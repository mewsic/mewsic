<config>
	<host><%= APPLICATION[:url] %></host>
	<fms><%= APPLICATION[:fms_url] %></fms>
	<media><%= APPLICATION[:media_url] %></media>
	<current_user><%= current_user.id if logged_in? %></current_user>
	<url_request>
		<mylist>/users/{:id}/mlabs</mylist>
		<search>/search/new.xml</search>
		<song>/songs/{:id}.xml</song>
		<song_mix>/songs/{:id}/mix.xml</song_mix>
		<track>/tracks/{:id}</track>
		<instruments>/instruments.xml?</instruments>
		<genres>/genres.xml</genres>
		<trackediting>/songs/{:id}.xml</trackediting>
		<trackdelete>/tracks/delete/{:id}</trackdelete>
		<trackupload>/tracks</trackupload>
		<user>/user/{:id}.xml</user>
		<authenticity_token><%= form_authenticity_token if protect_against_forgery? %></authenticity_token>
	</url_request>
</config>
