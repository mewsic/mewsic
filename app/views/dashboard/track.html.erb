<html xmlns="http://www.w3.org/1999/xhtml">
<head>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
  <!-- <meta http-equiv="refresh" content="5;url=http://myousica.com" /> -->
  <link rel="shortcut icon" href="/favicon.ico" type="image/vnd.microsoft.icon"/>
</head>
<body style="margin:0;padding:0">
  <div style="width:100%;height:100%;text-align:center">
    <div style="position:absolute;top:10%;width:100%;">
      <%= image_tag 'spinner.gif' %> <em>Loading ...</em>
    </div>
    <div style="position:absolute;top:50%;width:100%;">
      <%= image_tag 'logo_myousica.gif' %>
    </div>
  </div>
  <%= google_analytics_load %>
  <%= google_analytics_track "track_#{escape_javascript @origin}" %>
  <%= javascript_tag 'window.location = "/"' %>
</body>
</html>
